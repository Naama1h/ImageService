@model ImageServiceWeb.Models.ImageWebModel
@{
    ViewBag.Title = "ImageWeb";
}

<h2>Image Web Welcome</h2>

<p>
    
</p>

<table class="table">

    <tr>
        <th>First Name</th>
        <th>Last Name</th>
        <th>ID</th>
        <th></th>
    </tr>
    @foreach (var item in Model.students)
    {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.FirstName)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.LastName)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.ID)
            </td>
        </tr>
    }

</table>

